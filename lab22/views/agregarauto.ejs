<%- include('partials/head.ejs') %>
<body>
	<%- include('partials/navbar.ejs') %>
	<h1 class="display-1 text-center"><%= titulo %></h1>
		<br>
		 <form class="mx-auto row g-3" action="agregarauto?_csrf=<%=csrfToken%>" method="POST" enctype="multipart/form-data">
		 	<div class="mx-auto row g-3">
				<div class="col">
					<input type="text" name="marca" class="form-control" placeholder="Brand" aria-label="First name">
				</div>
				<div class="col">
					<input type="text" name="modelo" class="form-control" placeholder="Model" aria-label="Last name">
				</div>
				<div class="col-12">
					<label for="imagen_auto" class="form-label">Imagen</label>
					<input type="file" name="imagen_auto" class="form-control" placeholder="Picture" aria-label="picture">
				</div>
				<div class="col-12 mx-auto">
					<input class="btn btn-primary" type="submit" value="Guardar">
				</div>
			</div>
		 </form>
	<footer>
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.bundle.min.js" integrity="sha384-b5kHyXgcpbZJO/tY9Ul7kGkf1S0CWuKcCD38l8YkeH8z8QjE0GmW1gYU5S9FOnJ0" crossorigin="anonymous"></script>
	</footer>
</body>
</html>